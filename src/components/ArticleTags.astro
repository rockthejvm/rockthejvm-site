---
import type { CollectionEntry } from "astro:content";
import ArticleTag from "./ArticleTag.astro";

interface Props {
  tags: CollectionEntry<"articles">["data"]["tags"];
}
const { tags } = Astro.props;
---

<div>
  {tags.map((tag) => <ArticleTag {tag} />)}
</div>
<style>
  div {
    display: flex;
    flex-wrap: wrap;
  }
</style>
