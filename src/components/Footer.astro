---
import { Icon } from "astro-icon/components";
import FooterHelper from "./FooterHelper";
---

<FooterHelper>
  <Icon
    name="socials/facebook"
    title="Facebook"
    aria-hidden="true"
    class="h-6 w-6"
    slot="facebook"
  />
  <Icon
    name="socials/github"
    title="GitHub"
    aria-hidden="true"
    class="h-6 w-6"
    slot="github"
  />
  <Icon
    name="socials/linkedin"
    title="LinkedIn"
    aria-hidden="true"
    class="h-6 w-6"
    slot="linkedin"
  />
  <Icon
    name="socials/twitter"
    title="Twitter"
    aria-hidden="true"
    class="h-6 w-6"
    slot="twitter"
  />
  <Icon
    name="socials/youtube"
    title="YouTube"
    aria-hidden="true"
    class="h-6 w-6"
    slot="youtube"
  />
  <Icon
    name="socials/rss"
    title="RSS"
    aria-hidden="true"
    class="h-6 w-6"
    slot="rss"
  />
</FooterHelper>
